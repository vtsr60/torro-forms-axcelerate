{
	"name": "vtsr60/torro-forms-axcelerate",
	"description": "Torro Forms Plugin to create contact in aXcelerate SMS.",
	"version": "1.0.1",
	"license": "GPL-2.0-or-later",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress",
		"plugin",
		"extension",
		"torro forms",
		"forms",
		"form builder",
		"aXcelerate",
		"api"
	],
	"homepage": "https://github.com/vtsr60/torro-forms-axcelerate",
	"authors": [
		{
			"name": "Raj Vivakaran",
			"email": "vtsr60@gmail.com",
			"homepage": "https://github.com/vtsr60"
		}
	],
	"support": {
		"issues": "https://github.com/awsmug/torro-forms-axcelerate/issues"
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.6.0",
		"composer/installers": "~1.0",
		"api-api/structure-wordpress": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": ">4.8.20 <6.0",
		"squizlabs/php_codesniffer": "3.*",
		"wp-coding-standards/wpcs": "^1",
		"wimg/php-compatibility": "^8",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
	},
	"scripts": {
		"install-codestandards": [
			"Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
		]
	}
}
